{
  "Rules": [
    {
      "ID": "TransitionToStandardIA",
      "Status": "Enabled",
      "Filter": {
        "Prefix": "Replace-Me-With-Prefix/"
      },
      "Transitions": [
        {
          "Days": 30,
          "StorageClass": "STANDARD_IA"
        }
      ],
      "Expiration": {
        "Days": 90
      },
      "NoncurrentVersionExpiration": {
        "NoncurrentDays": 90
      },
      "AbortIncompleteMultipartUpload": {
        "DaysAfterInitiation": 7
      }
    }
  ]
}
